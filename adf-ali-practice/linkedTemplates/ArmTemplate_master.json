{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ali-practice"
		},
		"ls_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_SQL'"
		},
		"ls_fs_01_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_fs_01'"
		},
		"ls_storage_file_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_storage_file'"
		},
		"ls_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfdbpractice.database.windows.net"
		},
		"ls_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "practicedatabase"
		},
		"ls_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsaad"
		},
		"ls_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_fs_01_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Alifiles"
		},
		"ls_fs_01_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "aliqasim.am7@outlook.com"
		},
		"ls_storage_file_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpracticeadlsgen.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ali-practice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_SQL_password": {
						"value": "[parameters('ls_SQL_password')]"
					},
					"ls_storage_file_accountKey": {
						"value": "[parameters('ls_storage_file_accountKey')]"
					},
					"ls_fs_01_password": {
						"value": "[parameters('ls_fs_01_password')]"
					},
					"ls_SQL_properties_typeProperties_server": {
						"value": "[parameters('ls_SQL_properties_typeProperties_server')]"
					},
					"ls_SQL_properties_typeProperties_database": {
						"value": "[parameters('ls_SQL_properties_typeProperties_database')]"
					},
					"ls_SQL_properties_typeProperties_userName": {
						"value": "[parameters('ls_SQL_properties_typeProperties_userName')]"
					},
					"ls_api_properties_typeProperties_url": {
						"value": "[parameters('ls_api_properties_typeProperties_url')]"
					},
					"ls_storage_file_properties_typeProperties_url": {
						"value": "[parameters('ls_storage_file_properties_typeProperties_url')]"
					},
					"ls_fs_01_properties_typeProperties_host": {
						"value": "[parameters('ls_fs_01_properties_typeProperties_host')]"
					},
					"ls_fs_01_properties_typeProperties_userId": {
						"value": "[parameters('ls_fs_01_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-ali-practice_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-ali-practice_ArmTemplate_0')]"
			]
		}
	]
}